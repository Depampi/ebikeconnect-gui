<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jaxb:bindings
        xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_1.xsd"
        version="2.1">

    <jaxb:globalBindings>
        <jaxb:javaType
                name="java.time.LocalDateTime"
                xmlType="xs:dateTime"
                parseMethod="de.perdoctus.ebikeconnect.jaxb.LocalDateTimeAdapter.parse"
                printMethod="de.perdoctus.ebikeconnect.jaxb.LocalDateTimeAdapter.print"/>
    </jaxb:globalBindings>


</jaxb:bindings>